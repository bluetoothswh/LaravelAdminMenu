{
    "name": "laramall/laravel-admin-menu",
    "type": "package",
    "description": "基于laravel5.5的 后台管理菜单的package",
    "keywords": ["laravel5.5", "laravle后台组件", "admin menu"],
    "homepage": "https://github.com/bluetoothswh/LaravelAdminMenu.git",
    "license": "MIT",
    "authors": [
        {
            "name": "David Sun",
            "email": "bluetooth_swh@163.com",
            "homepage": "https://larashuo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "LaraMall\\Admin\\Menu": "src/"
        }
    },
    "minimum-stability": "dev",
     "extra": {
        "laravel": {
            "providers": [
                "LaraMall\\Admin\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "LaraMall\\Admin\\Menu\\Facades\\Menu"
            }
        }
    }
}
